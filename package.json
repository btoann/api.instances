    "json-server": "^0.17.3",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "morgan": "^1.10.0",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "src/**/*.{js,json,scss}": "prettier --single-quote --trailing-comma all --write"
