{
  "name": "javascript",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "prettier": "lint-staged",
    "build:sass": "node-sass src/resources/scss/ --output src/public/css/ --no-source-map",
    "watch:sass": "yarn run build:sass && node-sass -wr src/resources/scss/ -o src/public/css/",
    "watch": "npm-run-all --parallel watch:*",
    "serve": "nodemon --inspect src/index.js",
    "dev": "npm-run-all --parallel watch serve",
    "start": "npm-run-all --parallel serve",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
